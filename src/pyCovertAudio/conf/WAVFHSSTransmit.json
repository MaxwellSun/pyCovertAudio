{
  "transmitter" :
  {
    "class"       : "WAVTransmitter.WAVTransmitter",
    "attributes"  :
    {
      "bitsPerSymbol"         : 1,
      "sampleRate"            : 48000,
      "samplesPerSymbol"      : 480,
      "symbolExpansionFactor" : 9,
      "separationIntervals"   : 2,

      "WAVInfo" :
      {
        "fileName"          : "transmit.wav",
        "numberOfChannels"  : 2,
        "channelMode"       : "blank"
      },

      "modulator" :
      {
        "class"       : "FHSSModulator.FHSSModulator",
        "attributes"  :
        {
          "minimumFrequency"              : 18000,
          "maximumFrequency"              : 20000,
          "bandwidthDivisor"              : 1.0
        }
      },

      "modifiers" :
      [
        {
          "class"       : "BandpassFilter.BandpassFilter",
          "attributes"  :
          {
            "firstStopband"       : 17800,
            "firstPassband"       : 18000,
            "secondPassband"      : 20000,
            "secondStopband"      : 22000,
            "passbandAttenuation" : 0.1,
            "stopbandAttenuation" : 80
          }
        }
      ],
  
      "dataInfo"  :
      {
        "data"      : "Hello",
        "sentinel"  : "UUW",
        "modifiers" :
        [
          {
            "class"       : "ReedSolomonEncoder.ReedSolomonEncoder",
            "attributes"  :
            {
              "messageLength" : 16,
              "blockLength"   : 32
            }
          },
          {
            "class"       : "ManchesterEncoder.ManchesterEncoder",
            "attributes"  :
            {
            }
          }
        ]
      }
    }
  },

  "debugDirectory"  : "tmp",
  "debugEnabled"    : 1,
  "outputDirectory" : "output"
}
